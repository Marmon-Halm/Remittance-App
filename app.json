 {
  "expo": {
    "name": "trucktransportsystem",
    "slug": "trucktransportsystem",
    "version": "1.0.0",
    "scheme": "com.trucktransportsystem.com",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/remer.png",
      "resizeMode": "cover",
      "backgroundColor": "#016e96"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trucktransportsystem.com"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.trucktransportsystem.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
 
}
